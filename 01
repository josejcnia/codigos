    <script>
        document.addEventListener('input', function(e) {
            if (e.target.classList.contains('puntaje')) {
                calcularPuntajes();
            }
        });

        function calcularPuntajes() {
            let dimensiones = {
                'aspectos_formales': { sum: 0, count: 0 },
                'preparacion_entrevista': { sum: 0, count: 0 },
                'desarrollo_entrevista': { sum: 0, count: 0 },
                'informacion_cliente': { sum: 0, count: 0 },
                'comprension_informacion': { sum: 0, count: 0 }
            };

            document.querySelectorAll('.puntaje').forEach(function(input) {
                if (input.checked) {
                    let dimension = input.dataset.dimension;
                    let value = parseFloat(input.value) || 0;
                    dimensiones[dimension].sum += value;
                    dimensiones[dimension].count++;
                    input.parentElement.parentElement.querySelector('.nota').value = value;
                }
            });

            let sumaPonderada = 0;
            let countDimensiones = 0;
            for (let dim in dimensiones) {
                if (dimensiones[dim].count > 0) {
                    let promedio = dimensiones[dim].sum / dimensiones[dim].count;
                    document.getElementById('promedio_' + dim).textContent = promedio.toFixed(2);
                    sumaPonderada += promedio;
                    countDimensiones++;
                } else {
                    document.getElementById('promedio_' + dim).textContent = 'N/A';
                }
            }

            let calificacionFinal = countDimensiones > 0 ? (sumaPonderada / countDimensiones) : 0;
            // Convertir la calificación a escala de 1 a 7
            calificacionFinal = ((calificacionFinal - 1) / 3) * 6 + 1;
            document.getElementById('calificacion_final').textContent = calificacionFinal.toFixed(1);
        }
    </script>
</body>
</html>
